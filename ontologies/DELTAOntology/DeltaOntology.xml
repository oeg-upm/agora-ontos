    <!-- http://delta.linkeddata.es/def/core#capacitySold -->


    <owl:DatatypeProperty rdf:about="http://delta.linkeddata.es/def/core#capacitySold">
        <rdfs:label>capacity sold</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://delta.linkeddata.es/def/core#hasBeginning -->


    <owl:DatatypeProperty rdf:about="http://delta.linkeddata.es/def/core#hasBeginning">
        <rdfs:label>has beginning</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://delta.linkeddata.es/def/core#hasEnd -->


    <owl:DatatypeProperty rdf:about="http://delta.linkeddata.es/def/core#hasEnd">
        <rdfs:label>has end</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://delta.linkeddata.es/def/core#hasTimestamp -->


    <owl:DatatypeProperty rdf:about="http://delta.linkeddata.es/def/core#hasTimestamp">
        <rdfs:label>has timestamp</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://delta.linkeddata.es/def/core#stateOfCharge -->


    <owl:DatatypeProperty rdf:about="http://delta.linkeddata.es/def/core#stateOfCharge">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
        <rdfs:label>state of charge</rdfs:label>
    </owl:DatatypeProperty>
    


